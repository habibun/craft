{% for p in purchase %}
    <tr>
        <td>{{ p.purchaseDate|date('d/m/Y') }}</td>
        <td>{{ p.supplier }}</td>
        <td>{{ p.location }}</td>
        <td>
            {% if p.status %}
                <span class="label label-inverse">Finalized</span>
            {% else %}
                <span class="label label-warning">Not finalized</span>
            {% endif %}
        </td>
    </tr>
{% else %}
    <tr>
        <td>Unable to find Purchase entity.</td>
    </tr>
{% endfor %}